{
  "expo": {
    "name": "HrtApp",
    "slug": "HrtApp",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/preload/icon.png",
    "splash": {
      "image": "./assets/preload/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/preload/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lml.transformation",
      "buildNumber": "1.0.2",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses your camera to take photos and store them on the app. We do not collect this data."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/preload/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/preload/adaptive-icon.png"
    }
  }
}
